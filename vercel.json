{
    "version": 2,
    "source": [
        {
            "src": "./main.ts",
            "use": "@vercel/node"
        }
    ],
    "destination": [
        {
            "src": "/(.*)",
            "dest": "./main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}